    "electron-debug": "^1.1.0",
    "file-exists": "^2.0.0",
    "font-awesome": "^4.4.0",
    "formsy-material-ui": "^0.5.2",
    "formsy-react": "^0.18.1",
    "immutable": "^3.7.6",
    "material-design-iconic-font": "^2.2.0",
    "material-icons": "^0.1.0",
    "material-ui": "^0.16.1",
