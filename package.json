{
  "name": "sauce_debug_client",
  "version": "1.1.0",
  "description": "REST Client for Sauce Labs day to day",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "build": "cross-env NODE_ENV=production  webpack --config ./webpack.config.js  --progress --profile --colors",
    "build:dev": "cross-env NODE_ENV=development  webpack --config ./webpack.config.js  --progress --profile --colors",
    "start:dev": "cross-env NODE_ENV=development electron .",
    "start": "cross-env NODE_ENV=production electron app",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot -d --inline --port 3000",
    "dist": "build --appDir app -d"
  },
  "repository": "halkeye/sauce-debug-client",
  "author": "Gavin Mogan <gavin@gavinmogan.com>",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.12.3",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.3.0",
    "chromedriver": "^2.19.0",
    "co-mocha": "^1.1.2",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron-complete-builder": "^0.7.0",
    "electron-prebuilt": "^1.4.5",
    "electron-rebuild": "^1.0.0",
    "eslint": "^3.9.1",
    "eslint-config-semistandard": "git+https://github.com/jokeyrhyme/eslint-config-semistandard.git#bump-deps-cull-peers",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-standard": "^2.0.1",
